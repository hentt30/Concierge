{
   "type": "sqlite",
   "database": "database.sqlite",
   "strict": false,
   "synchronize": true,
   "logging": false,
   "entities": ["dist/**/*.entity.js"],
   "migrations": [
      "src/migration/**/*.ts"
   ],
   "subscribers": [
      "src/subscriber/**/*.ts"
   ],
   "cli": {
      "entitiesDir": "src/entity",
      "migrationsDir": "src/migration",
      "subscribersDir": "src/subscriber"
   }
}